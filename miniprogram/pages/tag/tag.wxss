.tag-icon{
  margin-right: 10px;
  margin-top: 10px;
  overflow: hidden;
  position: fixed;
  z-index: 100;
  right: 20rpx;
  top: 90rpx;
}
.tag-icon>text{
  font-size: 26px;
  font-weight: bold;
  border:2px solid #f05842;
  width: 30px;
  height: 30px;
  background: white;
  line-height: 28px;
  border-radius: 50%;
  margin-bottom: 20rpx;
  float: right;
  text-align: center;
  color: #f05842;
}
/* 主体部分 */
.main-area{
  width: 730rpx;
  height: 110rpx;
  margin:10rpx auto;
  /* background: pink; */
}
.move-area{
  width: 100%;
  height: 110rpx;
  margin-bottom: 20rpx;
  /* background: green; */
  /* margin-right: 20rpx; */
}
.move-view{
  width: 940rpx;
  height: 110rpx;
  display: flex;
  justify-content: space-between;
  /* background: palegoldenrod; */
}
.left-area{
  display: flex;
  /* margin-right: 16rpx; */
  align-items: center;
  color: #444;
  font-weight: 600;
  width: 728rpx;
  height: 108rpx;
  border-radius: 10rpx;
  border: 1rpx solid #ccc;
  box-shadow: 1px 1px 3px #ccc;
}
.left-area>.img-logo{
  width: 74rpx;
  height: 74rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  margin-left: 16rpx;
  border:1rpx solid skyblue;
}
.right-area{
  display: flex;
  height: 108rpx;
  align-items: center;
  justify-content: space-around;
  width: 190rpx;
  text-align: center;
}
.editor{
 background: orange;
 height: 104rpx;
 width: 50%; 
 line-height: 104rpx;
 color: white;
 border-top-left-radius: 10rpx;
 border-bottom-left-radius: 10rpx;
}
.del{
 background: #ff4800;
 height: 104rpx;
 width: 50%;
 line-height: 104rpx;
 color: white;
 border-top-right-radius: 10rpx;
 border-bottom-right-radius: 10rpx; 
}
/* 主题部分 */








/* 对话框 */
.dialog-area{
  position: fixed;
  top: 0px;
  left: 0px;
  /* background: #ccc; */
  z-index: 1000;
}
.hidden-area{
  position: fixed;
  z-index: 2000;
  width: 100%;
  height: 100%;
  top: 0px;
  left: 0px;
  bottom: 0px;
  /* background: green; */
  background: rgba(0,0,0,0.5);
}
.mian-area{
  position: fixed;
  z-index: 3000;
  background: white;
  padding: 20rpx;
  width: 640rpx; 
  height: 530rpx;
  left: 50%;
  margin-left: -340rpx;
  top: 50%;
  margin-top: -265rpx;
}
.dialog-title{
  display: flex;
  margin-bottom: 40rpx;
}
.title-input{
  margin-left: 20rpx;
}
.choice-tag{
  display: flex;
}
.logo-num{
  margin-left: 20rpx;
  
}
.log-area{
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.log-area .log-circle{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%; 
  justify-content: space-between;
  margin: 14rpx; 
}
.btn-area{
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
}
.cancel{
  width: 120px;
  height: 30px;
  line-height: 30px;
  border:2px solid #f84052;
  text-align: center;
  color: #f84052;
}
.acertain{
  width: 120px;
  height: 30px;
  line-height: 30px;
  border:2px solid #f84052;
  background: #f84052;
  text-align: center;
  color: white;
}
