.dialog-area{
  position: fixed;
  top: 0px;
  left: 0px;
  /* background: #ccc; */
  z-index: 1000;
}
.hidden-area{
  position: fixed;
  z-index: 2000;
  width: 100%;
  height: 100%;
  top: 0px;
  left: 0px;
  bottom: 0px;
  /* background: green; */
  background: rgba(0,0,0,0.5);
}
.mian-area{
  position: fixed;
  z-index: 3000;
  background: white;
  padding: 20rpx;
  width: 640rpx; 
  height: 530rpx;
  left: 50%;
  margin-left: -340rpx;
  top: 50%;
  margin-top: -265rpx;
}
.dialog-title{
  display: flex;
  margin-bottom: 40rpx;
}
.title-input{
  margin-left: 20rpx;
}
.choice-tag{
  display: flex;
}
.logo-num{
  margin-left: 20rpx;
  
}
.log-area{
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.log-area .log-circle{
  width: 100rpx;
  border-radius: 50%; 
  justify-content: space-between;
  margin: 14rpx; 
}
.btn-area{
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
}
.cancel{
  width: 120px;
  height: 30px;
  line-height: 30px;
  border:2px solid #f84052;
  text-align: center;
  color: #f84052;
}
.acertain{
  width: 120px;
  height: 30px;
  line-height: 30px;
  border:2px solid #f84052;
  background: #f84052;
  text-align: center;
  color: white;
}
